<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>YouTube Downloader</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>

<body>
  <nav class="navbar navbar-expand-lg bg-body-tertiary" data-bs-theme="dark">
    <div class="container">
      <a class="navbar-brand" href="#">You<span class="badge bg-danger">Tube</span></a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

    </div>
  </nav>
  <!--Heading Details-->
  <div class="my-5 text-center">
    <h1>YouTube-<span class="badge bg-danger">Audio/Video</span>-Downloader</h1>
  </div>
  <!--ending-->
  <!--input url-->

  <form action="/url" method="post">
    <div class="input-group mb-3 container">
      <input type="text" class="form-control"
        placeholder="Enter YouTube Link url  Ex:--https://youtu.be/wr9M-CoxP7A?si=k2Yjadjsdj"
        aria-label="Recipient's username" aria-describedby="basic-addon2" name="inpurl">
      <button type="submit" class="input-group-text btn btn-success" id="basic-addon2">Download</button>
    </div>
  </form>
        <!--input url ending-->

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
          integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
          crossorigin="anonymous"></script>
</body>

</html>